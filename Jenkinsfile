pipeline {
    agent any
    stages {
        stage("test") {
            steps {
                script {
                    echo "Run some test..."
                }
            }
        }
        stage("build") {
            steps {
                script {
                    echo "building..."
                }
            }
        }
        stage("deploy") {
            steps {
                script {
                    echo "deploying..."
                }
            }
        }
    }
}
